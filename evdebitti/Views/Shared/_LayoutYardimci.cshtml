    <script type="text/javascript">
            $(function() {
                    $('.sepeteekle').on('click', function() {
                         var id=$(this).attr("data-uruid");
                         var urunadi=$(this).attr("data-urunadi");
                         var urunfiyat=$(this).attr("data-urunfiyat");
                         var urunresim=$(this).attr("data-urunres");

                         $.post("/Home/sepeteekle?URUNID="+id+"&urunadi="+urunadi+"&urunfiyat="+urunfiyat+"&urunresimyol="+urunresim+"&quantity=1", function(data) {
                                 $.ajax({
                                 type:"GET",
                                 url:"/Home/sepetbilgileri",
                                     success:function Verigetir(data){
                                     $(".header-cart").html('');
                                     $(".header-cart").html(data);
                                     }
                                });
                                             // Get the modal
                                    var modal = document.getElementById("myModal");

                                    // Get the button that opens the modal
                                    var btn = document.getElementById("myBtn");

                                    // Get the <span> element that closes the modal
                                    var span = document.getElementsByClassName("close")[0];

                                    // When the user clicks on the button, open the modal
       
                                      modal.style.display = "block";
        

                                    // When the user clicks on <span> (x), close the modal
                                       setTimeout(salise,2000);

                                       function salise() {

                                       modal.style.display="none";

                                       }
                                     
                                    // When the user clicks anywhere outside of the modal, close it
                                    window.onclick = function(event) {
                                      if (event.target == modal) {
                                        modal.style.display = "none";
                                      }
                                    }
                            });
                       return false;
                    });
            });
